{
  "name": "@todovue/tvcard",
  "private": false,
  "author": "Cristhian Daza",
  "description": "Component Card created for the TodoVue blog",
  "license": "MIT",
  "version": "1.0.1",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/TODOvue/todovue-card.git"
  },
  "bugs": {
    "url": "https://github.com/TODOvue/todovue-card/issues"
  },
  "keywords": [
    "todovue",
    "front-end",
